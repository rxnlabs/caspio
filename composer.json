{
    "name": "rxnlabs/caspio",
    "description": "A un-official PHP Library to access and use the Caspio REST API",
    "require": {
        "php": ">=5.3.0",
        "nategood/httpful": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rxnlabs/caspio.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:nategood/httpful.git"
        }
    ],
    "license": "MIT",
    "keywords": ["caspio", "crm", "rest", "library", "api"],
    "version": "v0.1.1",
    "authors": [
        {
            "name": "rxnlabs",
            "email": "admin@rxnlabs.com",
            "homepage": "http://rxnlabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Caspio\\": "src/"
        }
    }
}
