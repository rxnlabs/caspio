{
    "name": "rxnlabs/caspio",
    "description": "A un-official PHP Library to access and use the Caspio REST API",
    "type": "library",
    "authors": [
        {
            "name": "De'Yonte Wilkinson",
            "email": "git@rxnlabs.com",
            "homepage": "https://rxnlabs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2",
        "nategood/httpful": "*@dev",
        "guzzlehttp/guzzle": "6.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rxnlabs/caspio.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:nategood/httpful.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:guzzle/guzzle.git"
        }
    ],
    "license": "MIT",
    "keywords": ["caspio", "crm", "rest", "library", "api"],
    "version": "v0.1.1",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Caspio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caspio\\": "tests"
        }
    }
}
